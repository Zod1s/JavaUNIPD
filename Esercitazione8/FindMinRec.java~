import java.util.*;
public class FindMinRec{
    public static void main(String[] args){
        if (args[0] == null || args[1] == null || args.length > 2){
            throw new IllegalArgumentException();
        }
        else{
            int dim = Integer.parseInt(args[0]);
            int n = Integer.parseInt(args[1]);
        }
    }
}
